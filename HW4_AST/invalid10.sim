(* array type expression not a constant that folds *)

PROGRAM X;


	VAR
		x: INTEGER;


	CONST
		z = 1;
		y = 10 DIV 2 + z; (* 6 *)


	TYPE
		Q = RECORD
			z: INTEGER;
		END;

		A = ARRAY 5 + x OF INTEGER;



BEGIN


	x:= 0 + y;

	WHILE x+1 <= 2 DO
		WRITE y + 2 * y
	END;

	x := -1 * (-20) + 6 DIV 2
	

END X.